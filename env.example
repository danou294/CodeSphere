# Configuration des variables d'environnement pour CodeSphere
# Copiez ce fichier vers .env.local et remplissez avec vos vraies valeurs

# Backend API
VITE_API_BASE_URL=https://codesphere-back-74d9cea0c732.herokuapp.com/api

# Stripe (clé publique - peut être exposée)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# Firebase (clés publiques - peuvent être exposées)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
